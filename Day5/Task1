let nums = [5, 12, 8, 20, 30];

let bigNums = nums.filter(n => n > 10);
console.log(bigNums);

let sum = nums.reduce((acc, val) => acc + val);
console.log(sum);

let sliced = nums.slice(1, 4);
console.log(sliced);

nums.splice(2, 1, 99);
console.log(nums);

let copied = [...nums];
let merged = [...nums, ...[40, 50]];
console.log(copied);
console.log(merged);